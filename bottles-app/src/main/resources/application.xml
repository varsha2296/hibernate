<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean class="com.xworkz.bottles.Bottles" id="bottles">
		<property name="bottles">
			<map>
				<entry key="Milton" value="ThermosteelBottle"></entry>
				<entry key="Prestige" value="CopperBottle"></entry>
				<entry key="Cello" value="PlasticBottle"></entry>
			</map>
		</property>
	</bean>
	<bean class="com.xworkz.bottles.Colors" id="colors">
		<property name="colors">
			<set>
				<value>Red</value>
				<value>Blue</value>
				<value>White</value>
			</set>
		</property>
	</bean>
	<bean class="com.xworkz.bottles.Shapes" id="shapes">
		<property name="shapesList">
			<list>
				<value>Square</value>
				<value>Round</value>
				<value>Cylindrical</value>
			</list>
		</property>
	</bean>
</beans>